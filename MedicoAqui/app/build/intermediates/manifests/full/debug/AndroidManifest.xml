<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gabriela.medicoaqui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="27" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.gabriela.medicoaqui.Activity.TelaPrincipal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.gabriela.medicoaqui.Activity.TelaLogin"
            android:label="Login" />
        <activity
            android:name="com.example.gabriela.medicoaqui.Activity.TelaCadastro"
            android:label="Cadastro" />
        <activity
            android:name="com.example.gabriela.medicoaqui.Activity.MedicoAqui"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.gabriela.medicoaqui.Activity.VisualizarHistorico"
            android:label="@string/title_activity_visualizar_historico"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>